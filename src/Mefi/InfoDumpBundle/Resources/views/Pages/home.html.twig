{% extends 'MefiInfoDumpBundle::page.html.twig' %}

{% block stylesheets %}
  {{ parent() }}
  {% stylesheets  output="generated/home.css"
  'bundles/mefiinfodump/less/home.less' filter='less' filter='cssrewrite'
  %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %}
{% endblock %}

{% block page_javascripts %}

  {% javascripts output="generated/home.js"
  'bundles/mefiinfodump/js/*.js'
  %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}
{% endblock %}

{% block content %}

  <div class="container">
    <div class="row">
      <div class="span3"> <!-- sidebar -->
        {% block leftnav %}
          {% render controller('MefiInfoDumpBundle:Common:leftnav', {}) %}
        {% endblock %}
      </div><!--/span-->
      <div class="span9"> <!--Body content-->
        {%  block template %}
          {% render controller('MefiInfoDumpBundle:Home:template', {}) %}
        {% endblock %}
      </div>
    </div>
  </div>
{% endblock %}