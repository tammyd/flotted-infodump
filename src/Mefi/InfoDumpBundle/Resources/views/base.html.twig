<!DOCTYPE html>
{% block htmltag %}
<html>
{% endblock %}
<head>
  {% block headcontent  %}

    {% block stylesheets %}
      {% stylesheets  output="generated/infodump.css"
      'bootstrap/less/bootstrap.less' filter='less' filter='cssrewrite'
      'bootstrap/less/responsive.less' filter='less' filter='cssrewrite'
      %}
      <link rel="stylesheet" href="{{ asset_url }}" />
      {% endstylesheets %}
    {% endblock %}

    {% block global_javascripts %}
      <script type="text/javascript" src="http://code.jquery.com/jquery-1.8.3.min.js"></script>
      {%  javascripts output="generated/infodump.js"
        'bundles/mefiinfodump/flot/jquery.flot.js'
        'bundles/mefiinfodump/flot/jquery.flot.time.js'
        'bundles/mefiinfodump/flot/jquery.flot.selection.js'
        'bundles/mefiinfodump/flot/jquery.flot.stack.js'
      %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
    {% endblock %}
    {% block page_javascripts %}
    {% endblock %}
  {% endblock %}


</head>
<body>

{% block body %}
  <div id="wrap">
      {% block bodycontent %}
        BodyContent w/in wrqp
      {% endblock %}
  </div>
  <div id="footer">
    {%  block footercontent %}
      Sticky Footer
    {%  endblock %}
  </div>


  {% block footer_javascripts %}
    {#Note that jquery must come before twitter bootstrap js, and twitter bootstrap order is important.#}
    {% javascripts output="generated/bootstrap.js"
    'bootstrap/js/bootstrap-transition.js'
    'bootstrap/js/bootstrap-alert.js'
    'bootstrap/js/bootstrap-modal.js'
    'bootstrap/js/bootstrap-dropdown.js'
    'bootstrap/js/bootstrap-scrollspy.js'
    'bootstrap/js/bootstrap-tab.js'
    'bootstrap/js/bootstrap-tooltip.js'
    'bootstrap/js/bootstrap-popover.js'
    'bootstrap/js/bootstrap-button.js'
    'bootstrap/js/bootstrap-collapse.js'
    'bootstrap/js/bootstrap-carousel.js'
    'bootstrap/js/bootstrap-typeahead.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

  {% endblock  %}
{% endblock %}
</body>
</html>